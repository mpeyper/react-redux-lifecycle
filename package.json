{
  "name": "react-redux-lifecycle.",
  "version": "1.0.0",
  "description": "A higher-order component to dispatch actions on react lifecycle methods",
  "author": "Michael Peyper",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/mpeyper/react-redux-lifecycle.git"
  },
  "scripts": {
    "start": "npm run test:watch",
    "dist": "babel src --out-dir lib --copy-files",
    "lint": "eslint src/ --ext .js --ext .jsx",
    "lint:fix": "eslint src/ --ext .js --ext .jsx --fix",
    "test": "JEST_JUNIT_OUTPUT=./test-reports/test-output.xml jest --coverage",
    "test:watch": "npm test -- --watch",
    "prepublish": "npm run dist"
  },
  "dependencies": {
    "react-redux": "^5.0.4"
  },
  "peerDependencies": {
    "react": ">= 15.1"
  },
  "devDependencies": {
    "@types/react": "^15.0.0",
    "babel-cli": "^6.18.0",
    "babel-core": " ^6.7.7",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.16.1",
    "eslint-plugin-react": "^7.1.0",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "jest-junit": "^1.2.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.6.0",
    "redux-mock-store": "^1.2.1",
    "typescript": "^2.1.5",
    "typescript-definition-tester": "0.0.5"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "testResultsProcessor": "./node_modules/jest-junit"
  }
}
